{
  "name": "mobile-travel-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/mobile-travel-e2e/src",
  "implicitDependencies": ["mobile-travel"],
  "// targets": "to see all targets run: nx show project mobile-travel-e2e --web",
  "targets": {
    "e2e": {
      "executor": "@nx/playwright:playwright",
      "outputs": ["{workspaceRoot}/dist/.playwright/apps/mobile-travel-e2e"],
      "options": {
        "config": "apps/mobile-travel-e2e/playwright.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  }
}
